fieldDefinition(filed) ::= <<
<if(filed.alias)><filed.alias>: <endif><filed.name><if(filed.arguments)><filed.arguments><endif><if(filed.directives)> <filed.directives:{directive|<directive>};separator=" "><endif><if(filed.selections)> {
    <filed.selections:{selection|<selection>};separator="\n">
}<endif>
>>
